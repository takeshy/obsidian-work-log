    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "^1.10.0",
    "tslib": "^2.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.18.2"
  }
}
